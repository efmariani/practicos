    <h1>Agrega tu producto</h1>
    <div class="formDiv">
        <form action="" method="post">
            <label for="postTitulo">Titulo</label>
            <input id="postTitulo" type="text" name="title">
    
            <label for="POST-SKU">SKU (Opcional)</label>
            <input id="POST-SKU" type="text" name="SKU">
    
            <label for="POST-description">Descripci√≥n</label>
            <input id="POST-description" type="textbox" name="description">

            <label for="POST-price">Precio A$R </label>
